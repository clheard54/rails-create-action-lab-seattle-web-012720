<!-- Create your form here -->

<%= form_tag students_path do %>
    <h1>Student Form</h1>
    <%= label_tag 'First Name:' %><br>
    <%= text_field_tag 'first_name' %><br><br>


    <%= label_tag 'Last Name:' %><br>
    <%= text_field_tag 'last_name' %><br><br>

    <%= submit_tag 'Submit Student' %>
<% end %>